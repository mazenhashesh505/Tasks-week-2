#include <iostream>
using namespace std;
int
main ()
{
  int num;
  int a, b, c, d, e;
  cin >> num >> a >> b >> c >> d >> e;
  int count = 0;

  if (a <= num)
	count += 1;

  if (b <= num)
	count += 1;

  if (c <= num)
	count += 1;

  if (d <= num)
	count += 1;

  if (e <= num)
	count += 1;

  cout << count << " " << 5 - count;

  return 0;
}
